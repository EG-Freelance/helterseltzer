<!-- Dropdown Structure -->
<ul id="dropdown1" class="dropdown-content">
  <%= @games.each do |g| %>
    <li><%= link_to g.date.try(:strftime, "%F") + "vs. " + g.game, g %></li>
  <% end %>
</ul>
<nav>
  <div class="nav-wrapper">
    <ul class="left hide-on-med-and-down">
      <li><%= link_to "HOME", root_path %></li>
      <li><%= link_to "NEW POST", new_post_path %></li>
      <!-- Dropdown Trigger -->
      <li><a class="dropdown-trigger" href="#!" data-target="dropdown1">GAMES LIST<i class="material-icons right">arrow_drop_down</i></a></li>
    </ul>
  </div>
</nav>

<script>
  $(document).ready(function(){
    $(".dropdown-trigger").dropdown();
  })
</script>